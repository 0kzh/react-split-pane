{
  "parser": "babel-eslint",
  "env": {
    "node": false,
    "browser": true,
    "es6": true
  },
  "rules": {
    "strict": [2, "global"],
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-extra-parens": 0,
    "react/jsx-uses-vars": 1
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "eslint-plugin-react"
  ]
}